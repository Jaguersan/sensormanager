<h1>Listing concentradors</h1>
<div id="tabla2">
<table>
  <thead>
    <tr>
      <th class="th">Categoria equipo</th>
      <th class="th">Nombre</th>
      <th class="th">Descripcion</th>
      <th class="th">Estado</th>
      <th class="th">Gis latitud</th>
      <th class="th">Gis longitud</th>
      <th class="th">Mac address</th>
      <th class="th">Numero serie</th>
      <th class="th">Ip address</th>
      <th class="th">Ip port</th>
      <th class="th"></th>
      <th class="th"></th>
      <th class="th"></th>
      <th class="th"></th>
    </tr>
  </thead>

  <tbody>
    <% @concentradors.each do |concentrador| %>
      <tr>
        <td><%= concentrador.categoria_equipo.nombre %></td>
        <td><%= concentrador.nombre %></td>
        <td><%= concentrador.descripcion %></td>
        <td><%= concentrador.estado %></td>
        <td><%= concentrador.gis_latitud %></td>
        <td><%= concentrador.gis_longitud %></td>
        <td><%= concentrador.mac_address %></td>
        <td><%= concentrador.numero_serie %></td>
        <td><%= concentrador.ip_address %></td>
        <td><%= concentrador.ip_port %></td>
        <td><%= link_to 'Show', concentrador %></td>
        <td><%= link_to 'Edit', edit_concentrador_path(concentrador) %></td>
        <td><%= link_to 'Destroy', concentrador, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td><%= link_to 'Sensores', sensores_concentrador_path(concentrador) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>

<%= link_to 'New Concentrador', new_concentrador_path %>
<%= link_to 'Galeria de Imagenes', galeria_path %>
