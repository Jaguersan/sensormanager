<p id="notice"><%= notice %></p>


<h1>Mostrando Estaciones para la Categoria "<%= @categoria_equipo.nombre %>"</h1>
<div id="tabla2">
<table>
  <thead>
    <tr>
      <th class="th">Nombre</th>
      <th class="th">Descripcion</th>
      <th class="th">Estado</th>
      <th class="th">Gis latitud</th>
      <th class="th">Gis longitud</th>
      <th class="th">Mac address</th>
      <th class="th">Numero serie</th>
      <th class="th">Ip address</th>
      <th class="th">Ip port</th>
    </tr>
  </thead>

  <tbody>
    <% @categoria_equipo.concentradors.each do |concentrador| %>
      <tr>
        <td><%= concentrador.nombre %></td>
        <td><%= concentrador.descripcion %></td>
        <td><%= concentrador.estado %></td>
        <td><%= concentrador.gis_latitud %></td>
        <td><%= concentrador.gis_longitud %></td>
        <td><%= concentrador.mac_address %></td>
        <td><%= concentrador.numero_serie %></td>
        <td><%= concentrador.ip_address %></td>
        <td><%= concentrador.ip_port %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
<br>
<%= link_to 'Back', categoria_equipos_path %>